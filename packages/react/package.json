{
  "name": "@hai3/react",
  "version": "0.3.0-alpha.0",
  "description": "React bindings and hooks for HAI3 framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "CLAUDE.md",
    "llms.txt",
    "commands"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.2",
    "react": "^19.2.4",
    "@types/react": "^19.0.8",
    "react-redux": "^9.1.0",
    "@types/react-redux": "^7.1.33",
    "vitest": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@testing-library/react": "^16.1.0",
    "@reduxjs/toolkit": "^2.5.0",
    "jsdom": "^25.0.1"
  },
  "peerDependencies": {
    "@hai3/framework": "*",
    "react": "^19.2.4",
    "react-redux": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "@hai3/framework": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-redux": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "hai3",
    "react",
    "hooks",
    "provider",
    "typescript"
  ],
  "author": "HAI3",
  "license": "Apache-2.0"
}
